<% content_for :title do %>
  Knack Tour
<% end %>
<% content_for :head do %>
  <%= javascript_include_tag "/javascripts/jquery.cycle.all.min.js"  %>
  <%= javascript_include_tag "/javascripts/jquery.easing.1.3.js"  %>
  <%= javascript_include_tag "/javascripts/knack.info.tour.js"  %>
  <%= javascript_include_tag "/javascripts/jquery.fancybox-1.3.1.js"  %>
  <%= stylesheet_link_tag "https://fonts.googleapis.com/css?family=Raleway:100"  %>
  <%= stylesheet_link_tag "/stylesheets/Pages/Tour.css" %>
  <%= stylesheet_link_tag "/stylesheets/jquery.fancybox-1.3.1.css" %>
  <meta http-equiv="X-UA-Compatible" content="IE=EmulateIE7">

<%  end %>

<div id="content" >
  <div class="div-c">
    <table id="container-image">
      <tr>
        <td>
          <ul id="knack_slideshow">
            <li id="support_small_start"></li>
            <li id="support_small"><a href="/tour/unique_registry" id="small_business">Create Unique Registry</a></li>
            <li id="share_wishlist_start"></li>
            <li id="share_wishlist"><a href="/tour/share_your_wishlist" id="share_wishlist_popup">Share Wishlist</a></li>
            <li id="add_gifts_start"></li>
            <li id="add_gifts"><a href="/tour/add_gifts_from_anywhere" id="add_gifts_from_anywhere">Add gifts from anywhere</a></li>
            <li id="fund_your_start"></li>
            <li id="fund_your"><a href="/tour/fund_your_honeymoon" id="fund_your_honeymoon">Fund your Honeymoon</a></li>
            <li id="collect_your_start"></li>
            <li id="collect_your"><a href="/tour/collect_your_gifts" id="collect_your_gifts">Collect your Gifts</a></li>
          </ul>
          <div id="lines_slideshow"></div>
        </td>
      </tr>
    </table>
  </div>
</div>
